<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="css/style.css">
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <header>
        <img src="img/the_simpsons.png" alt="">
    </header>
    <section>
        <%results.forEach(data=>{%>
        <div>
      
          <img src="<%=data.image%>" alt=""> 
          <p><%=data.quote%></p>
          <p class="character"><%=data.character%></p>
            <p><%=data.characterDirection%></p>

            <form action="/addFav" method="POST">
                <input type="hidden" name="quote" value="<%=data.quote%>">
                <input type="hidden" name="character" value="<%=data.character%>">
                <input type="hidden" name="image" value="<%=data.image%>">
                  <input type="hidden" name="characterDirection" value="<%=data.characterDirection%>"> 
                  <button type="submit">Add To Favorit</button> 
                </form>
        </div>

        <%})%>
    </section>
</body>
</html>